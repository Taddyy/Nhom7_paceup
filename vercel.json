{
  "functions": {
    "api/index.py": {
      "includeFiles": "backend/**"
    },
    "api/test.py": {}
  },
  "rewrites": [
    {
      "source": "/api/v1/:path*",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/test",
      "destination": "/api/test.py"
    }
  ],
  "version": 2
}
