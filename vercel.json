{
  "functions": {
    "api/index.py": {
      "includeFiles": "backend/**"
    },
    "api/debug.py": {
      "runtime": "@vercel/python"
    }
  },
  "rewrites": [
    {
      "source": "/api/debug",
      "destination": "/api/debug"
    },
    {
      "source": "/debug",
      "destination": "/api/debug"
    },
    {
      "source": "/api/v1/:path*",
      "destination": "/api/index"
    },
    {
      "source": "/api/test",
      "destination": "/api/test"
    }
  ],
  "version": 2
}
