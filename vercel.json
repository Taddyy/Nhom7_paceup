{
  "functions": {
    "api/index.py": {
      "includeFiles": "backend/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/test-minimal",
      "destination": "/api/test-minimal"
    },
    {
      "source": "/api/debug-lambda",
      "destination": "/api/debug-lambda"
    },
    {
      "source": "/api/debug-asgi",
      "destination": "/api/debug-asgi"
    },
    {
      "source": "/api/debug",
      "destination": "/api/debug"
    },
    {
      "source": "/debug",
      "destination": "/api/debug"
    },
    {
      "source": "/api/test",
      "destination": "/api/test"
    },
    {
      "source": "/api/v1/:path*",
      "destination": "/api/index"
    }
  ],
  "version": 2
}
